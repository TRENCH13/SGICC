<AnchorPane prefHeight="723.0" prefWidth="783.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.sgicc.Controllers.PerifericosController">
    <children>
        <Label layoutX="23.0" layoutY="25.0" text="Sistema de Gestión de Centros de Cómputo" textFill="#295ba5">
            <font>
                <Font size="24.0" />
            </font>
        </Label>
        <TableView fx:id="tvPerifericos" layoutY="88.0" prefHeight="567.0" prefWidth="783.0" style="-fx-background-color: white; -fx-border-radius: 0;" stylesheets="@tableview.css">
            <columns>
                <TableColumn fx:id="tcCodigo" prefWidth="70.0" text="Código" />
                <TableColumn fx:id="tcTipo" prefWidth="112.0" text="Tipo" />
                <TableColumn fx:id="tcNombre" prefWidth="223.0" text="Nombre" />
                <TableColumn fx:id="tcEspecificaciones" prefWidth="270.0" text="Especificaciones" />
                <TableColumn fx:id="tcEstado" prefWidth="104.0" text="Estado" />
            </columns>
        </TableView>
        <Button fx:id="btnEliminar" layoutX="689.0" layoutY="676.0" mnemonicParsing="false" onAction="#btnEliminar" text="Eliminar">
