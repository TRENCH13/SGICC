<AnchorPane prefHeight="122.0" prefWidth="347.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.sgicc.Controllers.SeleccionarNuevoEstadoController">
